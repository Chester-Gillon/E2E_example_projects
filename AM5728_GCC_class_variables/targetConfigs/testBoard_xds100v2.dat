# config version=3.5
$ sepk
  pod_drvr=jioserdesusb.dll
  pod_port=0
$ /
$ product
  title="Texas Instruments XDS100v2 USB"
  alias=TI_XDS100v2_USB
  name=FTDI_FT2232
$ /
